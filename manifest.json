{
  "name": "ImgMdConverter",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "ImgMdConverter",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus"
  ]
}